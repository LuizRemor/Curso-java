package Davi;

import java.util.ArrayList;

public class Exemplo {

	public static void main(String[] args) {
		
		ArrayList<String> lista = new ArrayList<>();

		lista.add("Luiz");
		lista.add("André");
		lista.add("Michael");
		
		System.out.println("=============================");
		
		for (String item : lista) {
			
			System.out.println(item);
			
		}
		
		System.out.println("=============================");

	}

}
